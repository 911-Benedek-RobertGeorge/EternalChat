    "@helia/json": "^3.0.4",
    "@metamask/eth-sig-util": "^7.0.3",
    "@web3-react/core": "^8.2.3",
    "axios": "^1.7.2",
    "crypto-js": "^4.2.0",
    "eth-crypto": "^2.6.0",
    "ethereumjs-util": "^7.1.5",
    "helia": "^4.2.4",
    "multiformats": "^13.1.3",
    "node-forge": "^1.3.1",
